	file = {Fahner - Developing Transparent Credit Risk Scorecards More.pdf:C\:\\Users\\kaleb\\Zotero\\storage\\44GE4446\\Fahner - Developing Transparent Credit Risk Scorecards More.pdf:application/pdf},
