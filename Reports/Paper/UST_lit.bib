@article{guignet_contamination_2018,
	title = {Contamination and incomplete information: Bounding implicit prices using high-profile leaks},
	volume = {88},
	issn = {00950696},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0095069616303886},
